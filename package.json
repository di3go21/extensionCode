{
  "name": "newsletter-sisnet-front-end",
  "displayName": "newsletter sisnet front-end",
  "description": "a newsletter for sisnet devs",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:newsletter-sisnet-front-end.newsLetterFront-End",
    "onCommand:newsletter-sisnet-front-end.newsLetterFront-End-Form"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "newsletter-sisnet-front-end.newsLetterFront-End",
        "title": "newsletter Front-End",
        "category": "SISnet Newsletter Front-End"
      },
      {
        "command": "newsletter-sisnet-front-end.newsLetterFront-End-Form",
        "title": "newsletter Front-End-Form",
        "category": "SISnet Newsletter Front-End"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
